void main(
	float2 in v_TexCoord00			: TEXCOORD0,

	float4 out Color				: COLOR,

	uniform sampler2D Texture00,

	uniform float AlphaThreshold
)
{
	Color = tex2D( Texture00, v_TexCoord00 );

	if( Color.w < AlphaThreshold ){
		discard;
	}
}
