void main(
	float2 in v_TexCoord00			: TEXCOORD0,

	float4 out Color				: COLOR,

	uniform float4 IDiffuse,

	uniform sampler2D Texture00
)
{
	Color = IDiffuse;

	float4 dstColor = Color;
	float4 texColor;

	texColor = tex2D( Texture00, v_TexCoord00 );

	Color = dstColor * texColor;
}
